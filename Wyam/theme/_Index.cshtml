<div class="register powered-by">
    <p class="endjin-link">powered by <a href="https://endjin.com" target="_blank"><strong>endjin.com</strong></a></p>
</div>

<div class="content2">
    <div class="row">
        <div class="contribute">
            <h2>Contribute</h2>
            <p>If you would like to contribute any content to the Power BI Weekly newsletter,</p>
            <p>please email <a href="&#109;&#097;&#105;&#108;&#116;&#111;:&#112;&#111;&#119;&#101;&#114;&#098;&#105;&#119;&#101;&#101;&#107;&#108;&#121;&#064;&#101;&#110;&#100;&#106;&#105;&#110;&#046;&#099;&#111;&#109;">&#112;&#111;&#119;&#101;&#114;&#098;&#105;&#119;&#101;&#101;&#107;&#108;&#121;&#064;&#101;&#110;&#100;&#106;&#105;&#110;&#046;&#099;&#111;&#109;</a></p>
        </div>
    </div>
</div>

<div class="content2">
@foreach(var contentPackage in Model.List<ContentPackage>("ContentPackages"))
{
    <div class="row">
        <div class="contribute">
            <h2>Issue @contentPackage.Issue:  @contentPackage.FromDate.ToString("dd/MM/yyyy") - @contentPackage.ToDate.ToString("dd/MM/yyyy")</h2>
            @Html.Raw(@contentPackage.Editorial)
        </div>
    </div>
}
